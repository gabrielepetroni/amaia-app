{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"amaia": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"standalone": false,
					"style": "scss"
				},
				"@schematics/angular:directive": {
					"standalone": false
				},
				"@schematics/angular:pipe": {
					"standalone": false
				}
			},
			"root": "",
			"sourceRoot": "src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:application",
					"options": {
						"outputPath": "dist/amaia",
						"index": "src/index.html",
						"browser": "src/main.ts",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "tsconfig.app.json",
						"assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
						"styles": [
							"src/styles.scss",
							"node_modules/@nebular/theme/styles/prebuilt/default.css"
						],
						"scripts": []
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "4mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
              "outputHashing": "all",
              "serviceWorker": "ngsw-config.json"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "amaia:build:production"
						},
						"development": {
							"buildTarget": "amaia:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"buildTarget": "amaia:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "tsconfig.spec.json",
						"assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
						"styles": [
							"src/styles.scss",
							"node_modules/@nebular/theme/styles/prebuilt/default.css"
						],
						"scripts": []
					}
				},
				"deploy": {
					"builder": "@angular/fire:deploy",
					"options": {
						"version": 2
					},
					"configurations": {
						"production": {
							"buildTarget": "amaia:build:production",
							"serveTarget": "amaia:serve:production"
						},
						"development": {
							"buildTarget": "amaia:build:development",
							"serveTarget": "amaia:serve:development"
						}
					},
					"defaultConfiguration": "production"
				}
			}
		}
	},
	"cli": {
		"analytics": false
	}
}